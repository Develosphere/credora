# Python API URL (FastAPI backend)
PYTHON_API_URL=http://localhost:8000
NEXT_PUBLIC_PYTHON_API_URL=http://localhost:8000

# Java FPA Engine URL (Spring Boot)
JAVA_ENGINE_URL=http://localhost:8081
NEXT_PUBLIC_JAVA_ENGINE_URL=http://localhost:8081

# NextAuth Secret (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=your-secret-key-here-replace-in-production

# Google OAuth redirect URI (must match Google Console)
GOOGLE_AUTH_REDIRECT_URI=http://localhost:3000/api/auth/callback
